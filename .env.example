# API Base URL
# 백엔드 API의 기본 URL
VITE_API_BASE_URL=YOUR_API_URL

# OAuth2/OIDC 설정

# OAuth2 Authorization 엔드포인트 (필수)
# 예: https://auth.infoteam.com/authorize
VITE_OAUTH2_AUTHORIZATION_URL=

# OAuth2 Client ID (필수)
# OAuth2 클라이언트 등록 시 발급받은 Client ID
VITE_OAUTH2_CLIENT_ID=

# OAuth2 Redirect URI (필수)
# OAuth2 인증 후 리다이렉트될 Callback URL
# 개발 환경: http://localhost:5173/auth/callback
# 프로덕션 환경: https://yourdomain.com/auth/callback
VITE_OAUTH2_REDIRECT_URI=

# OAuth2 Scope (선택, 기본값: "openid profile email")
# 요청할 권한 범위를 설정합니다.
# 예: "openid profile email" 또는 "openid profile email offline_access"
VITE_OAUTH2_SCOPE=openid profile email

# OAuth2 Logout URL (선택)
# OAuth Provider 로그아웃 엔드포인트
# 설정하지 않으면 로컬 토큰만 삭제됩니다.
# 예: https://auth.infoteam.com/logout
VITE_OAUTH2_LOGOUT_URL=
